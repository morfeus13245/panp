{% extends 'base.html.twig' %}



{% block body %}






<table class="table table-striped table-inverse table-responsive">
    
        <tbody>
            <tr>
               
{{ form_start(form) }}
                <td style="font-size:45px;">{{form_label(form.nombre)}}</td>
                <td style="font-size:45px;">{{form_widget(form.nombre)}}</td>
            </tr>
            <tr>                
                <td style="font-size:45px;">{{form_label(form.apellido)}}</td>
                <td style="font-size:45px;">{{form_widget(form.apellido)}}</td>
            </tr>
            <tr>                
                <td style="font-size:45px;">{{form_label(form.telefono)}}</td>
                <td style="font-size:45px;">{{form_widget(form.telefono)}}</td>
            </tr>
            <tr>                
                <td style="font-size:45px;">{{form_label(form.lugar)}}</td>
                <td style="font-size:45px;">{{form_widget(form.lugar)}}</td>

            </tr>
            <tr>                
                <td style="font-size:45px;"><div class=" text text-danger text-lg-center " id="labelinfo"></div></td>
                <td style="font-size:55px;">{{form_widget(form.submit)}}</td>

{{form_end(form)}}
            </tr>
        </tbody>
</table>

{% endblock %}

{% block javascripts %}

<script>
function funcion() {

  document.getElementById('cliente_submit').style.visibility = 'hidden';
  document.getElementById('labelinfo').innerHTML = "Procesando....";

  return true;
}
</script>


	
{% endblock %}
